{
	"indent": {
		"value": "  ",
    "MemberExpression": 0
	},
  "whiteSpace": {
    "after": {
      "FunctionReservedWord": 1
    },
    "before": {
      "PropertyName": 1
    }
  },
  "plugins": [
    "esformatter-collapse-objects",
    "esformatter-semicolons",
    "esformatter-braces",
    "esformatter-quotes",
    "esformatter-quote-props"
  ],
  "quotes": {
    "type": "single",
    "avoidEscape": false
  }
}
